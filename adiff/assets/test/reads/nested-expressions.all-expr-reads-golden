int min(int x, int y)
{
    __VERIFIER_read(x <= y, x, y);
    if (x <= y)
    {
        return x;
    }
    else
    {
        return y;
    }
}
int max(int x, int y)
{
    __VERIFIER_read(x >= y, x, y);
    if (x >= y)
    {
        return x;
    }
    else
    {
        return y;
    }
}
int add(int x, int y)
{
    return x + y;
}
int main()
{
    int x = 1;
    int y = 2;
    int * arr[3];
    __VERIFIER_read(x, y);
    if (min(x, y) > 2)
    {
        ;
    }
    __VERIFIER_read(x);
    if (arr[min(1, x)])
    {
    }
    __VERIFIER_read(arr[0] == 1);
    if (arr[0] == 1)
    {
    }
}
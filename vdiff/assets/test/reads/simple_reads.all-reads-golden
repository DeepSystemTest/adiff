void foo(int x)
{
}
void bar();
int main()
{
    int x, y, z;
    __VERIFIER_read(x);
    if (x > 0)
    {
        int a;
        __VERIFIER_read(foo);
        foo(3);
        __VERIFIER_read(y, y);
        y = y + 1;
        __VERIFIER_read(bar);
        bar();
        __VERIFIER_read(x, x);
        x = x + 1;
        __VERIFIER_read(z, z);
        z = z + 1;
    }
}
int main()
{
    int x, y, z;
    __VERIFIER_read(x < 0, x);
    if (x < 0)
    {
        __VERIFIER_read(y + z == 3, y + z, y, z);
        while (y + z == 3)
        {
            int z;
            __VERIFIER_read(z, z + 1, z);
            z = z + 1;
            for (int i = 0; i < 5; i++)
            {
                __VERIFIER_read(z, z + 3, z);
                z = z + 3;
            }
        }
    }
}
void foo(int x)
{
}
void bar();
int main()
{
    int x, y, z;
    __VERIFIER_read(x > 0, x);
    if (x > 0)
    {
        int a;
        foo(3);
        __VERIFIER_read(y + 1, y);
        y = y + 1;
        bar();
        __VERIFIER_read(x + 1, x);
        x = x + 1;
        __VERIFIER_read(z + 1, z);
        z = z + 1;
    }
}
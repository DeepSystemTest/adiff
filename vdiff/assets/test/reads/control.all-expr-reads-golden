int main()
{
    int x;
    __VERIFIER_read(x < 5, x);
    while (x < 5)
    {
        ;
    }
    __VERIFIER_read(x);
    for (int i = x; i < 5; i++)
    {
        ;
    }
    __VERIFIER_read(x > 5, x);
    if (x > 5)
    {
        ;
    }
    __VERIFIER_read(x);
    switch (x)
    {
    case 0:
        return 1;
    default:
        return 0;
    }
}